    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "rootDir": "./src",
        "outDir": "./lib"
    },
    "exclude": [
        "node_modules"
    ]
}
